{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel/laravel"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://portfolio-uzairmzln.vercel.app",
      "DB_CONNECTION": "mysql",
      "DB_HOST": "your-database-host",
      "DB_PORT": "3306",
      "DB_DATABASE": "your-database-name",
      "DB_USERNAME": "your-database-username",
      "DB_PASSWORD": "your-database-password"
    }
  }  